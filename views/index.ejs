<!DOCTYPE html>
<html>
  <head>
    <title>CSV Data</title>
  </head>
  <style>  table, th, td {
    border: 1px solid;
  }</style>

  <body>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Age</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach((row) => { %>
          <tr>
            <td><%= row.name %></td>
            <td><%= row.age %></td>
            <td><%= row.email %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>

